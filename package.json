{
  "name": "@jescrich/nestjs-toolkit",
  "version": "0.0.14",
  "description": "NestJS Toolkit",
  "license": "MIT",
  "author": "Jos√© Escrich",
  "main": "dist/index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "nest build",
    "clean": "del-cli -f \"./dist\" && del-cli -f \"./*.tsbuildinfo\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.cjs --no-watchman",
    "test:watch": "jest --watch --config jest.config.cjs --no-watchman",
    "test:cov": "jest --coverage --config jest.config.cjs --no-watchman",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prePack": "cp -r ./dist/* .",
    "prepublishOnly": "cp -r ./dist/* ."
  },
  "dependencies": {
    "@golevelup/ts-jest": "^0.6.1",
    "@keyv/redis": "^4.0.2",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^10.4.13",
    "@nestjs/core": "^10.4.13",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.4.13",
    "@nestjs/platform-express": "^10.4.13",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^10.0.1",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "cache-manager": "^6.3.0",
    "class-validator": "^0.14.1",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.15",
    "moment": "^2.29.4",
    "mongodb": "^6.11.0",
    "mongoose": "^8.8.4",
    "mongoose-paginate-v2": "^1.7.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.13",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.5.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "_moduleAliases": {
    "@this/*": "*"
  }
}
